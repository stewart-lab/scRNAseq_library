{
    "title": "scRNA-seq analysis",
    "annotation_reference": "FALSE",
    "DE_method": "Seurat",
    "lanes": [

    ],
    "prep_seurat_and_soupX": {
        "dims": 30,
        "umap.method": "uwot"
    },
    "filter_cells": {
        "lower.nFeature": 200,
        "upper.nFeature": 8000,
        "max.percent.mt": 5,
        "species": "human"
    },
    "normalize_data": {
        "min_size": 100,
        "min_mean": 0.1,
        "feature": "ECHS1"
    },
    "feature_selection": {
        "n_features": 2000,
        "analysis_type": "Scry"
    },
    "run_and_visualize_pca": {
        "top_n_dims": 2,
        "heatmap_dims": 15,
        "num_cells": 500
    },
    "perform_batch_correction": {
        "dims.use": 20,
        "max_iter": 50
    },
    "perform_clustering": {
        "save_obj_before_clustering": "TRUE",
        "num.replicate": 100,
        "dims": 20,
        "dims_umap": 15,
        "resolution": 0.14,
        "algorithm": "leiden",
        "umap.method": "uwot"
    },
    "find_differentially_expressed_features": {
        "min_pct": 0.25,
        "logfc_threshold": 0.25,
        "top_n": 30
    },
    "score_and_plot_markers": {
        "top_n_markers": 10,
        "known_markers": "TRUE",
        "known_markers_path": "./known_marker_lists/GammLab_Retinal-specific_genelist.txt"
    }
}